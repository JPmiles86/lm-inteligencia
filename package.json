{
  "name": "laurie-inteligencia",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx watch api/server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "vite build",
    "build:api": "tsx api/server.ts --build",
    "build:bypass": "vite build",
    "start:api": "node api/server.js",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/scripts/seedBlog.ts",
    "ai:seed": "tsx src/db/seeds/aiSeeds.ts",
    "ai:test": "tsx src/scripts/testAIDatabase.ts",
    "style-guides:populate": "tsx src/scripts/populateStyleGuides.ts",
    "style-guides:check": "tsx src/scripts/checkStyleGuides.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "k6 run tests/performance/load-test.js",
    "test:security": "jest --testPathPattern=__tests__/security",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security",
    "test:ci": "jest --coverage --watchAll=false",
    "test:setup": "tsx tests/setup/test-setup.ts",
    "playwright:install": "playwright install",
    "db:test:setup": "tsx tests/setup/test-database.ts",
    "db:test:teardown": "tsx tests/setup/test-teardown.ts",
    "test:api": "jest --testPathPattern=__tests__/integration/api",
    "test:memory": "node --expose-gc tests/performance/memory-test.js",
    "test:db:performance": "tsx tests/performance/db-performance.ts",
    "test:coverage:consolidated": "jest --coverage --coverageDirectory=coverage --testPathPattern='(__tests__|tests)/'",
    "coverage:check": "node scripts/check-coverage.js",
    "coverage:percentage": "node scripts/coverage-percentage.js",
    "db:migrate:test": "DATABASE_URL=postgresql://test_user:test_password@localhost:5432/ai_content_test npm run db:migrate",
    "db:seed:test": "DATABASE_URL=postgresql://test_user:test_password@localhost:5432/ai_content_test npm run ai:seed",
    "db:analyze-performance": "tsx scripts/analyze-db-performance.ts",
    "dev:test": "NODE_ENV=test npm run dev",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:api": "node scripts/generate-api-docs.js",
    "format:check": "prettier --check 'src/**/*.{js,ts,tsx}' 'api/**/*.{js,ts}' '__tests__/**/*.{js,ts,tsx}'",
    "format:fix": "prettier --write 'src/**/*.{js,ts,tsx}' 'api/**/*.{js,ts}' '__tests__/**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@google-cloud/storage": "^7.17.0",
    "@google/generative-ai": "^0.24.1",
    "@types/pg": "^8.15.5",
    "@types/react-datepicker": "^6.2.0",
    "@types/sharp": "^0.31.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "formidable": "^3.5.4",
    "framer-motion": "^12.4.4",
    "helmet": "^8.1.0",
    "lucide-react": "^0.513.0",
    "marked": "^16.2.1",
    "multer": "^2.0.2",
    "openai": "^5.15.0",
    "papaparse": "^5.4.1",
    "pg": "^8.16.3",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-datepicker": "^8.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-quill": "^2.0.0",
    "react-quill-new": "^3.6.0",
    "react-router-dom": "^6.21.1",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.3",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/cors": "^2.8.19",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.8",
    "@types/marked": "^5.0.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "msw": "^2.0.8",
    "nock": "^13.4.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.4",
    "typescript": "^5.0.2",
    "vite": "^5.0.8",
    "vite-plugin-compression": "^0.5.1",
    "wait-on": "^7.2.0"
  }
}
